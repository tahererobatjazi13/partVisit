<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.partsystem.partvisitapp.feature.main.home.ui.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">

        <action
            android:id="@+id/action_homeFragment_to_customerListFragment"
            app:destination="@id/customerListFragment" />
        <action
            android:id="@+id/action_homeFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
        <action
            android:id="@+id/action_homeFragment_to_headerOrderFragment"
            app:destination="@id/headerOrderFragment" />
        <action
            android:id="@+id/action_homeFragment_to_groupProductFragment"
            app:destination="@id/groupProductFragment" />
        <action
            android:id="@+id/action_homeFragment_to_ProductListFragment"
            app:destination="@id/productListFragment" />
        <action
            android:id="@+id/action_homeFragment_to_reportFactorFragment"
            app:destination="@id/reportFactorFragment" />
    </fragment>
    <fragment
        android:id="@+id/onlineOrderListFragment"
        android:name="com.partsystem.partvisitapp.feature.report_factor.ui.online.OnlineOrderListFragment"
        android:label="OnlineOrderListFragment"
        tools:layout="@layout/fragment_order_list">
        <argument
            android:name="typeList"
            app:argType="string" />
        <argument
            android:name="id"
            app:argType="integer" />
        <action
            android:id="@+id/action_onlineOrderListFragment_to_onlineOrderDetailFragment"
            app:destination="@id/onlineOrderDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/onlineOrderDetailFragment"
        android:name="com.partsystem.partvisitapp.feature.report_factor.ui.online.OnlineOrderDetailFragment"
        android:label="OnlineOrderDetailFragment"
        tools:layout="@layout/fragment_order_detail">
        <argument
            android:name="id"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/customerListFragment"
        android:name="com.partsystem.partvisitapp.feature.customer.ui.CustomerListFragment"
        android:label="CustomerListFragment"
        tools:layout="@layout/fragment_customer_list">
        <action
            android:id="@+id/action_customerListFragment_to_customerDetailFragment"
            app:destination="@id/customerDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.partsystem.partvisitapp.feature.setting.ui.SettingFragment"
        android:label="SettingFragment"
        tools:layout="@layout/fragment_setting" />
    <fragment
        android:id="@+id/customerDetailFragment"
        android:name="com.partsystem.partvisitapp.feature.customer.ui.CustomerDetailFragment"
        android:label="CustomerDetailFragment"
        tools:layout="@layout/fragment_customer_detail">
        <argument
            android:name="customerId"
            app:argType="integer" />
        <action
            android:id="@+id/action_customerDetailFragment_to_headerOrderFragment"
            app:destination="@id/headerOrderFragment" />
        <action
            android:id="@+id/action_customerDetailFragment_to_onlineOrderListFragment"
            app:destination="@id/onlineOrderListFragment" />
    </fragment>
    <fragment
        android:id="@+id/groupProductFragment"
        android:name="com.partsystem.partvisitapp.feature.group_product.ui.GroupProductFragment"
        android:label="GroupProductFragment"
        tools:layout="@layout/fragment_group_product">
        <argument
            android:name="fromFactor"
            app:argType="boolean" />
        <argument
            android:name="factorId"
            android:defaultValue="0"
            app:argType="reference" />

        <action
            android:id="@+id/action_groupProductFragment_to_productDetailFragment"
            app:destination="@id/productDetailFragment" />
        <action
            android:id="@+id/action_groupProductFragment_to_orderFragment"
            app:destination="@id/orderFragment" />
    </fragment>
    <fragment
        android:id="@+id/headerOrderFragment"
        android:name="com.partsystem.partvisitapp.feature.create_order.ui.HeaderOrderFragment"
        android:label="CustomerDetailFragment"
        tools:layout="@layout/fragment_header_order">
        <argument
            android:name="typeCustomer"
            app:argType="boolean" />
        <argument
            android:name="typeOrder"
            app:argType="string" />
        <argument
            android:name="customerId"
            app:argType="integer" />

        <argument
            android:name="customerName"
            app:argType="string" />

        <argument
            android:name="factorId"
            android:defaultValue="0"
            app:argType="reference" />

        <action
            android:id="@+id/action_headerOrderFragment_to_productListFragment"
            app:destination="@id/productListFragment" />
        <action
            android:id="@+id/action_headerOrderFragment_to_groupProductFragment"
            app:destination="@id/groupProductFragment" />
    </fragment>
    <fragment
        android:id="@+id/productListFragment"
        android:name="com.partsystem.partvisitapp.feature.product.ui.ProductListFragment"
        android:label="ProductListFragment"
        tools:layout="@layout/fragment_product_list">
        <action
            android:id="@+id/action_productListFragment_to_productDetailFragment"
            app:destination="@id/productDetailFragment" />

        <argument
            android:name="fromFactor"
            app:argType="boolean" />
        <argument
            android:name="factorId"
            android:defaultValue="0"
            app:argType="reference" />

        <action
            android:id="@+id/action_productListFragment_to_orderFragment"
            app:destination="@id/orderFragment" />
    </fragment>
    <fragment
        android:id="@+id/productDetailFragment"
        android:name="com.partsystem.partvisitapp.feature.product.ui.ProductDetailFragment"
        android:label="ProductDetailFragment"
        tools:layout="@layout/fragment_product_detail">
        <argument
            android:name="productId"
            app:argType="integer" />
    </fragment>

    <fragment
        android:id="@+id/orderFragment"
        android:name="com.partsystem.partvisitapp.feature.create_order.ui.OrderFragment"
        android:label="OrderFragment"
        tools:layout="@layout/fragment_order">
        <argument
            android:name="factorId"
            app:argType="integer" />
        <action
            android:id="@+id/action_orderFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/reportFactorFragment"
        android:name="com.partsystem.partvisitapp.feature.report_factor.ui.ReportFactorFragment"
        android:label="ReportFactorFragment"
        tools:layout="@layout/fragment_report_factor" />
    <action
        android:id="@+id/action_global_to_headerOrderFragment"
        app:destination="@id/headerOrderFragment" />

</navigation>
